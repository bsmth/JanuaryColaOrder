<!DOCTYPE html>
<html>
<head>
  <% include ../partials/header.ejs %>
</head>

<body>

<% include ../partials/nav.ejs %>

<div class="container">
<h2>Latest Results</h2>

<div class="table-responsive">
  <table class="table table-striped table-sm">
    <thead>
      <tr>
        <th>Timestamp</th>
        <th>Info</th>
      </tr>
    </thead>
    <tbody>
      <% results.slice().reverse().forEach(function(r) { %>
      <tr>
        <td><%= r.timestamp %></td>
        <td><%= r.description %></td>
      </tr>
      <% }); %>
    </tbody>
  </table>
</div>

<ul>

</ul>

</div>

</body>
</html>
